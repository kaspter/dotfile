set line /dev/ttyUSB0
set speed 115200
set carrier-watch off
set handshake none
set flow-control none
robust
set file type bin
set file name lit
set rec pack 1000
set send pack 1000
set window 5

.current := ~/log

.logfile := \v(ndate)-\v(time).log
.mode    := new


#set session-log text
set session-log debug
#set session-log timestamped-text

cd \m(current)
log session \m(logfile) \m(mode)
